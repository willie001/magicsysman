@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
	<title>Connection Validator - @DateTimeWrapper.DisplayLocalNow()</title>
</head>
<body>
	<h3>Connection Validator - @DateTimeWrapper.DisplayLocalNow()</h3>

	<div><pre>Connection string: @TempData["connstring"]</pre></div>
	<div><pre>Host: @TempData["host"]</pre></div>
	<div><pre>Environment: @TempData["environment"]</pre></div>
	<div><pre>@TempData["results"]</pre></div>

	<div><pre>Execution Time: @TempData["timer"]</pre></div>
	<div><pre>@TempData["debugger"]</pre></div>

</body>
</html>

