<form class="form-horizontal">
   	<div class="form-group">
      	<label class="col-md-2 control-label">Cleaner Code:</label>
      	<div class="col-md-10">
         	<input type="text" placeholder="1002" class="form-control" ng-readonly="true" />
      	</div>
  	</div>
   	<div class="form-group">
      	<label class="col-md-2 control-label">Initials:</label>
      	<div class="col-md-10">
         	<input type="text" placeholder="Cleaner Initials" class="form-control"  />
      	</div>
   	</div>
   	<div class="form-group">
      	<label class="col-md-2 control-label">Cleaner Name:</label>
      	<div class="col-md-10">
         	<input type="text" placeholder="Cleaner name" class="form-control" />
      	</div>
   	</div>
   	<div class="form-group">
      	<label class="col-md-2 control-label">Region:</label>
      	<div class="col-md-10">
         	<input type="text" placeholder="Metropolitan region" class="form-control" />
      	</div>
   	</div>
	<div class="form-group">
      	<label class="col-lg-2 control-label">Franchise</label>
      	<div class="col-lg-10">
         	<select name="account" class="form-control">
            	<option value="">Select Franchise</option>	
				<option value="NM">North Metro</option>
				<option value="SW">South West</option>
				<option value="TA">Tasmania</option>
      		</select>	
      	</div>
   	</div>
	<div class="form-group">
      	<label class="col-lg-2 control-label">Management Fee (%)</label>
      	<div class="col-lg-10">
         	<input type="text" placeholder="Management Fee" class="form-control" ng-ReadOnly="true"/>
      	</div>
   	</div>	
    <div class="form-group">
       	<div ng-controller="RatingController as rating">
          	<label class="col-md-2 control-label">Rating:</label>
          	<div class="col-md-10">
				<div class="stars">
		      		<input class="star star-5" id="star-5" type="radio" name="star"/>
			      	<label class="star star-5" for="star-5"></label>
			      	<input class="star star-4" id="star-4" type="radio" name="star"/>
			      	<label class="star star-4" for="star-4"></label>
			      	<input class="star star-3" id="star-3" type="radio" name="star"/>
			      	<label class="star star-3" for="star-3"></label>
			      	<input class="star star-2" id="star-2" type="radio" name="star"/>
			      	<label class="star star-2" for="star-2"></label>
			      	<input class="star star-1" id="star-1" type="radio" name="star"/>
			      	<label class="star star-1" for="star-1"></label>
			  	</div>
          	</div>
       	</div>
    </div>
	<div class="form-group">
		<label class="col-md-2 control-label">Leave dates:</label>
		<div class="col-md-10" ng-controller="DatepickerDemoCtrl as dp">
			<div class="col-md-1">From: </div>
			<div class="col-md-3">
				<p class="input-group">
				 	<input class="form-control" type="text" uib-datepicker-popup="{{dp.format}}" ng-model="dp.dt" is-open="dp.opened" min-date="dp.minDate" max-date="'2015-06-22'" datepicker-options="dp.dateOptions" date-disabled="dp.disabled(date, mode)"
                	ng-required="true" close-text="Close" />
                	<span class="input-group-btn">
                   		<button class="btn btn-default" type="button" ng-click="dp.open($event)">
                      		<em class="fa fa-calendar"></em>
                   		</button>
                	</span>
             	</p>
			</div>
			<div class="col-md-1"> to </div>
			<div class="col-md-3">
				<p class="input-group">
				 	<input class="form-control" type="text" uib-datepicker-popup="{{dp.format}}" ng-model="dp.dt" is-open="dp.opened" min-date="dp.minDate" max-date="'2015-06-22'" datepicker-options="dp.dateOptions" date-disabled="dp.disabled(date, mode)"
                	ng-required="true" close-text="Close" />
                	<span class="input-group-btn">
                   		<button class="btn btn-default" type="button" ng-click="dp.open($event)">
                      		<em class="fa fa-calendar"></em>
                   		</button>
                	</span>
             	</p>	
			</div>
			<div class="col-md-4">
				<button ng-click="$state.go('app.clients')" class="btn btn-primary">Find affected customers</button>
			</div>
		</div>
	</div>
   	<div class="form-group">
      	<label class="col-md-2 control-label">Active:</label>
  		<div class="col-md-10">
         	<label class="switch">
         		<input type="checkbox" checked="true" />
         		<span></span>
          	</label>
      	</div>
   	</div>
	<div class="panel panel-default">
		<div class="panel-heading">Team Details</div>
		<div class="panel-body">
			<div class="table-responsive">
				<table class="table table-bordered table-hover bg-white">
			      	<tr style="font-weight: bold">
			         	<td  class="text-center">Given Name</td>
			         	<td  class="text-center">Surname</td>
			         	<td  class="text-center">Address</td>
			         	<td  class="text-center">Mobile</td>
						<td  class="text-center">Email</td>
						<td  class="text-center">Gender</td>
						<td  class="text-center">Ironing</td>
						<td  class="text-center"></td>
			      	</tr>
					<tr>  
						<td>Alex</td>
						<td>Perry</td>
						<td>
							15 Somestreet,
							SomePlace
							WA, 6031
						</td>
						<td>0402 222 222</td>
						<td><span class="fa fa-envelope"></span>&nbsp;<a href="mailto:alex@tester.com">alex@tester.com</a></td>
						<td>
							<div class="radio c-radio c-radio-nofont">
                     			<label>
                        			<input type="radio" name="sex" value="M" checked="" />
                       				<span></span>Male
								</label>
                  			</div>
                  			<div class="radio c-radio c-radio-nofont">
                     			<label>
                        			<input type="radio" name="sex" value="F" />
                        			<span></span>Female
								</label>
                  			</div>
						</td>
						<td>
							<div class="radio c-radio c-radio-nofont">
                     			<label>
                        			<input type="radio" name="ironing" value="Y" checked="" />
                       				<span></span>Yes
								</label>
                  			</div>
                  			<div class="radio c-radio c-radio-nofont">
                     			<label>
                        			<input type="radio" name="ironing" value="N" />
                        			<span></span>No
								</label>
                  			</div>
						</td>
						<td>
							 <!-- form-->
				            <div ng-show="!rowform.$visible" class="buttons">
				               <button  title="Edit" class="btn btn-sm btn-info">
				                  <em class="fa fa-pencil"></em>
				               </button>
				               <button  title="Delete" class="btn btn-sm btn-danger">
				                  <em class="fa fa-trash"></em>
				               </button>
				            </div>
						</td>
					</tr>
				</table>
			</div>	<br/>
			<!-- buttons-->
	    	<button class="btn btn-primary">Add Team Member</button>
		</div>
	</div>

	<div class="form-group">
	    <div class="col-lg-offset-2 col-lg-6">
	    	
	    </div>
	    <div class="text-right col-lg-4">
			<button type="button" class="btn btn-default">Cancel</button>
			<button type="button" class="btn btn-success">Save</button>
		</div>
	</div>
</form>
